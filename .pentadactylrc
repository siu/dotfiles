:set go+=b
:autocmd LocationChange .* :js modes.passAllKeys = /mail\.google\.com/.test(buffer.URL)
:autocmd LocationChange .* :js modes.passAllKeys = /docs\.google\.com/.test(buffer.URL)
:autocmd LocationChange .* :js modes.passAllKeys = /www\.google\.com\/reader\/view/.test(buffer.URL)
:autocmd LocationChange .* :js modes.passAllKeys = /www\.hackpad\.com/.test(buffer.URL)
:noremap ; :
:noremap L gt
:noremap H gT
:noremap j 3j
:noremap k 3k

