
Xcursor*theme: Tango

!-------------------------------------------------------------------------------
!
!	U R X V T - Setting
!
URxvt*reverseVideo:		false

!URxvt*font: xft:Monospace:size=10:autohind=true
!Urxvt*boldFont: xft:Monospace:size=10:style=Bold:autohind=true

URxvt*font:             xft:Terminus:size=10:antialias=false
URxvt*boldFont:         xft:Terminus:style=Bold:size=10:antialias=false
URxvt*italicFont:       xft:Terminus:size=10:antialias=false
URxvt*bolditalicFont:   xft:Terminus:size=10:antialias=false

URxvt*cursorColor:	#d8d8d8
URxvt*colorBD:		lightyellow
URxvt*colorUL:		yellow

!colors
URxvt*background:		#333333
URxvt*foreground:		#ffffff

!black
URxvt*color0:			#676767
URxvt*color8:	        #757575
!red
URxvt*color1:	        #EA6868
URxvt*color9:	        #FF7272
!green
URxvt*color2:	        #ABCB8D
URxvt*color10:	        #AFD78A
!yellow
URxvt*color3:	        #E8AE5B
URxvt*color11:	        #FFA75D
!blue
URxvt*color4:	        #71C5F4
URxvt*color12:	        #67CDE9
!magenta
URxvt*color5:	        #E2BAF1
URxvt*color13:	        #ECAEE9
!cyan
URxvt*color6:	        #21F1EA
URxvt*color14:	        #36FFFC
!white
URxvt*color7:	        #F1F1F1
URxvt*color15:	        #FFFFFF

! Other customizations
URxvt*scrollBar:            true
URxvt*scrollBar_right:      true
URxvt*transpscrollbar:      true
URxvt*scrollBar_floating:   true
URxvt*saveLines:            10000
URxvt*scrollTtyOutput:      true
URxvt*scrollWithBuffer:     true
URxvt*scrollTtyKeypress:    true
URxvt*scrollstyle:          plain
URxvt*perl-lib:             /usr/lib/urxvt/perl
URxvt*perl-ext-common:      default,matcher
URxvt*urlLauncher:          firefox
URxvt*matcher.button: 1
URxvt.termname: rxvt
